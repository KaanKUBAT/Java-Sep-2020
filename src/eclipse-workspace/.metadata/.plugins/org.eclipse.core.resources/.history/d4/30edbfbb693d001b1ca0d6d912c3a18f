/*----------------------------------------------------------------------------------------------------------------------	
	Aşağıdaki örnekte import bildirimine ilişkin java dosyasında Sample sınıfı zaten olduğundan yani adeta bu java
	dosyası için Sample ismi rezerve erişmiş olduğundan aşağıdaki yıldızsız import bildirimi geçersizdir 
----------------------------------------------------------------------------------------------------------------------*/
package org.csystem.app;

import test.Sample; //error

class App {	
	public static void main(String [] args) 
	{
		Sample s = new Sample();
		
		s.foo();	
	}	
}